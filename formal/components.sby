[tasks]
executor all
decoder all
fetcher all
accessor all
writeback all
handshake all
skidbuffer all

[options]
all: mode prove

[engines]
all: smtbmc

[script]
decoder:
read -sv -formal structs.v decoder.v
prep -top decoder

fetcher:
read -sv -formal structs.v fetcher.v
prep -top fetcher

executor:
read -formal structs.v executor.v
prep -top executor

accessor:
read -formal accessor.v
prep -top accessor

writeback:
read -formal writeback.v
prep -top writeback

handshake:
read -formal handshake.v
prep -top handshake

skidbuffer:
read -formal -sv skidbuffer.v
prep -top skidbuffer
--
[files]
fetcher:
../rtl/fetcher.v
../rtl/structs.v

decoder:
../rtl/decoder.v
../rtl/structs.v

executor:
../rtl/executor.v
../rtl/structs.v

accessor:
../rtl/accessor.v

writeback:
../rtl/writeback.v

handshake:
../rtl/handshake.v

skidbuffer:
../rtl/skidbuffer.v
--
